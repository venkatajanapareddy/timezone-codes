{
  "name": "timezone-codes",
  "version": "1.0.0",
  "description": "A comprehensive, typed dataset of IANA timezone identifiers with metadata.",
  "type": "module",
  "author": "Venkata Phanindra Kumar Janapareddy",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/venkatajanapareddy/timezone-codes.git"
  },
  "bugs": {
    "url": "https://github.com/venkatajanapareddy/timezone-codes/issues"
  },
  "homepage": "https://github.com/venkatajanapareddy/timezone-codes#readme",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./timezones.json": "./dist/timezones.json"
  },
  "bin": {
    "timezone-codes": "./dist/cli.mjs"
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "dev": "vitest",
    "build": "tsup",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "prepare": "husky",
    "prepublishOnly": "npm run build && npm test"
  },
  "keywords": [
    "timezone",
    "iana",
    "time zone",
    "utc offset",
    "dst",
    "localization",
    "l10n",
    "internationalization",
    "i18n",
    "datetime",
    "typescript",
    "dataset"
  ],
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.11.24",
    "@typescript-eslint/eslint-plugin": "^7.1.0",
    "@typescript-eslint/parser": "^7.1.0",
    "eslint": "^8.57.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.0.0",
    "prettier": "^3.2.5",
    "tsup": "^8.0.2",
    "typescript": "^5.3.3",
    "vitest": "^1.3.1"
  },
  "lint-staged": {
    "*.{js,ts,jsx,tsx}": "eslint --fix",
    "*.{js,ts,jsx,tsx,json,md,yml}": "prettier --write"
  }
}
